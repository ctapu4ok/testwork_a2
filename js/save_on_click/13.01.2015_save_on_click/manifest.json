{
  "manifest_version": 2,
  "name":"Save image on click",
  "version": "1.0",
  "icons": {
    "48":"images/icon.png"
  },
  "options_page": "options.html",
  "browser_action": {
    "default_title": "Save image on click",
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },
  "background" : {
    "scripts" : ["background.js"]
  },
  "permissions": [
    "tabs",
    "activeTab",
    "background",
    "storage",
    "downloads",
    "*://*/*"
  ]
}